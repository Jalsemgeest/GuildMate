{
  "name": "guildmate",
  "version": "1.0.0",
  "description": "A site where you can plan and post for your guild.",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "copy-dist": "cp ./src/index.html ./dist/ && cp -a ./src/assets/. ./dist/assets/",
    "build": "npm run clean && npm run copy-dist & webpack",
    "clean": "rm -rf dist && mkdir -p ./dist/assets",
    "serve": "npm run clean && npm run copy-dist && webpack-dev-server --inline --history-api-fallback --content-base dist/"
  },
  "repository": {
    "type": "git",
    "url": "bitbucket.org:Jalsemgeest/guildmate.git"
  },
  "keywords": [
    "guild",
    "mate",
    "wow"
  ],
  "author": "Jake Alsemgeest",
  "license": "ISC",
  "dependencies": {
    "chart.js": "^2.0.0",
    "flux": "^2.1.1",
    "font-awesome": "^4.6.1",
    "font-awesome-webpack": "0.0.4",
    "jquery": "^2.2.2",
    "js-cookie": "^2.1.1",
    "moment": "^2.13.0",
    "react": "^0.14.7",
    "react-big-calendar": "^0.9.11",
    "react-dom": "^0.14.7",
    "react-router": "^2.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "bootstrap": "^3.3.6",
    "bootstrap-webpack": "0.0.5",
    "css-loader": "^0.23.1",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "imports-loader": "^0.6.5",
    "less": "^2.6.1",
    "less-loader": "^2.2.2",
    "node-sass": "^3.4.2",
    "postcss-loader": "^0.8.2",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14"
  }
}
